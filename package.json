{
  "name": "farmers-market-directory",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dbidmead.github.io/farmers-market-directory/",
  "scripts": {
    "dev": "npm run clean:dev && NODE_ENV=development next dev -p 3000",
    "build": "NODE_ENV=production next build && npm run copy-images",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:dev": "rm -rf .next node_modules/.cache",
    "copy-images": "node scripts/copy-images.js",
    "proxy": "nodemon server/server.js",
    "deploy": "node deploy-gh-pages.js"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "next": "^13.5.6",
    "nodemon": "^3.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.5.6",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.4"
  }
}
